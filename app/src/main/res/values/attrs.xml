<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GridInputText">
        <attr name="gridInputLength" format="integer" />
        <attr name="gridInputItemBackground" format="reference" />
        <attr name="gridInputHide" format="boolean" />
        <attr name="gridInputType" format="enum">
            <enum name="text" value="0" />
            <enum name="number" value="1" />
        </attr>
        <attr name="gridInputItemBackgroundType">
            <enum name="rect" value="0" />
            <enum name="line" value="1" />
            <enum name="self" value="2" />
        </attr>
        <attr name="gridInputItemLineBackgroundWidth" format="dimension|reference" />
        <attr name="gridInputItemLineBackgroundColor" format="color|reference" />
        <attr name="gridInputItemMargin" format="reference|dimension" />
        <attr name="gridInputIncludeParentMargin" format="boolean" />
        <attr name="gridInputTextSize" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="UIRate">
        <attr name="rateView_distance" format="dimension|reference" />
        <attr name="rateView_rateCount" format="integer" />
        <attr name="rateView_rate" format="float" />
        <attr name="rateView_step" format="float" />
        <attr name="rateView_fullSrc" format="reference" />
        <attr name="rateView_emptySrc" format="reference" />
        <attr name="rateView_srcHeight" format="dimension|reference" />
        <attr name="rateView_srcWidth" format="dimension|reference" />
        <attr name="rateView_editEnable" format="boolean" />
    </declare-styleable>
</resources>